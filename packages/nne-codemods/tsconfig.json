{
    "extends": "nne-tsconfig/tsconfig.json",
    "include": ["."],
    "exclude": ["./dist", "./build", "./node_modules"],
    "compilerOptions": {
        "allowJs": false,
        "declaration": true,
        "outDir": "dist",
        "declarationMap": false,
        "noImplicitAny": true,
        "strictNullChecks": false,
        "strict": false,
        "noEmitOnError": false
    }
  }
  