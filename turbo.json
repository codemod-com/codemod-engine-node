{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"lint": {
			"outputMode": "full",
			"dependsOn": ["^lint"]
		},
		"//#lint": {
			"dependsOn": [],
			"outputs": []
		},
		"build": {
			"dependsOn": ["lint", "//#lint", "^build"],
			"outputMode": "full"
		},
		"package": {
			"dependsOn": ["build", "^package"],
			"outputMode": "full"
		}
	}
}
